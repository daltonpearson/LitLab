{
    "manifest_version": 2,
    "name": "Lit Lab",
    "version": "0.0.0.1",
    "content_scripts": [
        {
        "matches": [
            "https://gitlab.com/*"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_start"
        }
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ],
    "web_accessible_resources": [
        "options.html",
        "options.js",
        "fonts.css",
        "normalize.css",
        "milligram.css"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_title": "Click here to open options",
        "default_popup": "options.html"
    }
}